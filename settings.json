{
    "vim.leader": "<space>",
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["d", "f"],
            "after": ["<Esc>"]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "i", "j"],
            "after": ["o", "<Esc>"]
        },
        {
            "before": ["<leader>", "i", "k"],
            "after": ["O", "<Esc>"]
        },
        {
            "before": [";"],
            "after": ["A", ";", "<Esc>"]
        }
    ]
}